{
    "name": "kreatinc/bot",
    "description": "building messenger bot from laravel package for laravel applications",
    "type": "laravel",
    "license": "MIT",
    "keywords": [
        "laravel",
        "package",
        "bot",
        "facebook"
    ],
    "authors": [
        {
            "name": "Kreatinc Team",
            "email": "hi@kreatinc.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "doctrine/dbal": "^2.9",
        "facebook/graph-sdk": "^5.7",
        "guzzlehttp/guzzle": "6.3.*",
        "illuminate/database": "~5.7.0|~5.8.0|^6.0|^7.0",
        "illuminate/http": "~5.7.0|~5.8.0|^6.0|^7.0",
        "illuminate/support": "~5.7.0|~5.8.0|^6.0|^7.0",
        "illuminate/console": "~5.7.0|~5.8.0|^6.0|^7.0",
        "illuminate/routing": "~5.7.0|~5.8.0|^6.0|^7.0",
        "illuminate/validation": "~5.7.0|~5.8.0|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Kreatinc\\Bot\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "dev"
        },
        "laravel": {
            "providers": [
                "Kreatinc\\Bot\\BotServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
